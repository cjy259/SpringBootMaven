<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bdqn.pojo.Feedbackfrom" table="FEEDBACKFROM" schema="SCOTT">
        <id name="id">
            <column name="ID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="feedback">
            <column name="FEEDBACK" sql-type="varchar2(255)" not-null="true"/>
        </property>
        <property name="summary">
            <column name="SUMMARY" sql-type="varchar2(255)" not-null="true"/>
        </property>
        <property name="examinationresults">
            <column name="EXAMINATIONRESULTS" sql-type="varchar2(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>