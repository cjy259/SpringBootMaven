<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bdqn.pojo.Bonus" table="BONUS" schema="SCOTT">
        <property name="ename">
            <column name="ENAME" sql-type="varchar2(10)" length="10" not-null="true"/>
        </property>
        <property name="job">
            <column name="JOB" sql-type="varchar2(9)" length="9" not-null="true"/>
        </property>
        <property name="sal">
            <column name="SAL" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="comm">
            <column name="COMM" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>