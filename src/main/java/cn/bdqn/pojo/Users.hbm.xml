<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bdqn.pojo.Users" table="USERS" schema="SCOTT">
        <id name="id">
            <column name="ID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="usercode">
            <column name="USERCODE" sql-type="varchar2(50)" length="50" not-null="true"/>
        </property>
        <property name="roleid">
            <column name="ROLEID" sql-type="varchar2(50)" length="50" not-null="true"/>
        </property>
        <property name="deptid">
            <column name="DEPTID" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="username">
            <column name="USERNAME" sql-type="varchar2(50)" length="50" not-null="true"/>
        </property>
        <property name="phone">
            <column name="PHONE" sql-type="char(11)" length="11" not-null="true"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar2(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>