<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.bdqn.pojo.Title" table="TITLE" schema="SCOTT">
        <id name="deptid">
            <column name="DEPTID" sql-type="number(*)" precision="2147483646"/>
        </id>
        <property name="titlecoding">
            <column name="TITLECODING" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="department">
            <column name="DEPARTMENT" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="titlename">
            <column name="TITLENAME" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="titledescription">
            <column name="TITLEDESCRIPTION" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="note">
            <column name="NOTE" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="disabled">
            <column name="DISABLED" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>